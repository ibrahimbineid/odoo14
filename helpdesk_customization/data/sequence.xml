<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
         Reference for PM
        <record id="ref_pm" model="ir.sequence">
            <field name="name">PM Reference</field>
            <field name="code">pm.ref</field>
            <field name="prefix">DT/PM/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <data noupdate="1">
         Reference for CM
        <record id="ref_cm" model="ir.sequence">
            <field name="name">CM Reference</field>
            <field name="code">cm.ref</field>
            <field name="prefix">DT/CM/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
